{
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/dashboard.html"
    },
    {
      "source": "/admin/books",
      "destination": "/kelola_buku.html"
    },
    {
      "source": "/admin/literacy",
      "destination": "/kelola_literasi.html"
    },
    {
      "source": "/admin/loans",
      "destination": "/kelola_peminjaman.html"
    },
    {
      "source": "/admin/students",
      "destination": "/kelola_siswa.html"
    },
    {
      "source": "/login",
      "destination": "/literacy_siswa.html"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}